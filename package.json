{
  "name": "pdf-to-markdown",
  "version": "0.1.3",
  "description": "Convert PDF to Markdown",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --mode development --watch",
    "build": "webpack",
    "start": "webpack serve",
    "lint": "eslint src --ext .ts,.tsx --cache",
    "lint:fix": "eslint src --ext .ts,.tsx --cache --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check": "npm run lint && npm run test",
    "release": "npm run lint && rm -rf build/* && NODE_ENV=production webpack --mode production",
    "deploy": "npm run release && cp -r build/* docs/"
  },
  "keywords": [
    "pdf",
    "markdown",
    "converter"
  ],
  "author": "Haijun Yang",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.15",
    "@types/react-bootstrap": "^1.1.0",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "pdfjs-dist": "^3.7.107",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.10.1",
    "react-overlays": "^5.2.1",
    "remarkable": "^2.0.1"
  }
}
